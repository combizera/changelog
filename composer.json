{
    "name": "combizera/changelog",
    "description": "A simple generator for Laravel projects.",
    "license": "MIT",
    "type": "library",
    "keywords": ["laravel", "changelog", "package"],
    "autoload": {
        "psr-4": {
            "Combizera\\Changelog\\": "src/"
        }
    },
    "authors": [
        {
            "name": "combizera",
            "email": "ycombi349@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "laravel/pint": "^1.22"
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Combizera\\Changelog\\ChangelogServiceProvider"
            ]
        }
    }
}
